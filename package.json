{
    "name": "api_with_client",
    "version": "1.0.0",
    "description": "api_with_client",
    "engines": {
        "node": "18.12.1"
    },
    "scripts": {
        "install-reactjs": "npm install --prefix reactjs",
        "install-api": "npm install --prefix api",
        "build-reactjs": "npm run build --prefix reactjs",
        "heroku-postbuild": "concurrently \"npm run install-reactjs\" \"npm run install-api\" \"npm run build-reactjs\"",
        "start": "npm run start --prefix api",
        "dev:reactjs": "concurrently \"npm run start --prefix reactjs\" \"npm run start --prefix api\"",
        "react": "cd reactjs && npm start",
        "api": "cd api; nodemon server.js"
    },
    "dependencies": {
        "concurrently": "^8.0.1",
        "express": "^4.17.1",
        "mongoose": "^5.13.2",
        "nodemon": "^2.0.12"
    }
}
